public class greatest{
	public static void main(String[] x)
	{
		int a=10;
		int b=20;
		int c=30;
		if(a>b)
			{
				if(a>c)
				{
					System.out.println("a is the greatest number");
				}
				else
				{
					System.out.println("c is the greatest number");
				}
			}
		else
			{
				if(b>c)
				{
					System.out.println("b is the greatest number");
				}
				else
				{
					System.out.println("c is the greatest number");
				}
			}
	}}
		
